<% include ../header.ejs%>
<header>
	<h2>Agenda de Contatos</h2>
</header>
<section>
	<form action="/conatct/<%- id %>?_method=put" method="post">
		<label>Nome: </label>
		<input type="text" name="contact[name]" value="<%- contact.name %>">
		<label>E-mail:</label>
		<input type="text" name="conatct[email]" value="<%- contact.email %>">
		<button type="submit">Atualizar</button>
	</form>
</section>

